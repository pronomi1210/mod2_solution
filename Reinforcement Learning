        "    axes[2].set_ylabel(\"Cumulative Reward\")\n",
        "else:\n",
        "    axes[2].axis('off')\n",
        "    axes[2].text(0.5, 0.5, \"Reward tracking not enabled\", ha='center', va='center', fontsize=12)\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n"
