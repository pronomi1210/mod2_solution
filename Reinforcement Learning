      "cell_type": "code",
      "source": [
        "# Multi Armed Bandit\n",
        "import numpy as np\n",
        "from sklearn.metrics import classification_report\n",
        "\n",
        "# Initialize\n",
        "n_actions = 2  # 0 = BENIGN, 1 = ATTACK\n",
        "action_values = np.zeros(n_actions)  # Estimated reward values\n",
        "action_counts = np.zeros(n_actions)  # How many times each action was taken\n",
        "\n",
        "#  Parameters\n",
        "epsilon = 0.1  # Exploration rate\n",
        "rewards = []\n",
        "y_pred = []\n",
        "\n",
        "# Loop through each sample\n",
        "for i in range(len(y)):\n",
        "    #  Choose action (explore or exploit)\n",
        "    if np.random.rand() < epsilon:\n",
        "        action = np.random.choice(n_actions)  # Explore\n",
        "    else:\n",
        "        action = np.argmax(action_values)     # Exploit best action\n",
        "\n",
        "    #  Reward: +1 for correct, -1 for wrong\n",
        "    reward = 1 if action == y[i] else -1\n",
        "    rewards.append(reward)\n",
        "    y_pred.append(action)\n",
        "\n",
        "    #  Update values\n",
        "    action_counts[action] += 1\n",
        "    alpha = 1 / action_counts[action]  # Learning rate\n",
        "    action_values[action] += alpha * (reward - action_values[action])\n",
        "\n",
        "# Evaluation\n",
        "print(\"‚úÖ Multi-Armed Bandit Classifier Evaluation:\")\n",
        "print(classification_report(y, y_pred, target_names=[\"BENIGN\", \"ATTACK\"]))\n"
      ],
      "metadata": {
