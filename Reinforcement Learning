      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "from sklearn.preprocessing import LabelEncoder\n",
        "\n",
        "#  Load combined file\n",
        "df = pd.read_csv(\"CIC_IDS2017_Combined.csv\")\n",
        "\n",
        "#  Rename Label column if needed\n",
        "if 'Label' not in df.columns:\n",
        "    df.rename(columns={df.columns[-1]: 'Label'}, inplace=True)\n",
        "\n",
        "#  Drop unnecessary columns\n",
        "cols_to_drop = ['Flow ID', 'Source IP', 'Destination IP', 'Timestamp', 'SimillarHTTP', 'Fwd Header Length.1']\n",
        "for col in cols_to_drop:\n",
        "    if col in df.columns:\n",
        "        df.drop(col, axis=1, inplace=True)\n",
        "\n",
        "#  Encode non-numeric columns (excluding Label)\n",
        "for col in df.columns:\n",
        "    if df[col].dtype == 'object' and col != 'Label':\n",
        "        df[col] = LabelEncoder().fit_transform(df[col].astype(str))\n",
        "\n",
        "#  Encode target: BENIGN=0, Attack=1\n",
        "df['Label'] = df['Label'].apply(lambda x: 0 if x == 'BENIGN' else 1)\n",
        "\n",
        "#  Clean data\n",
        "df = df.replace([np.inf, -np.inf], np.nan).dropna()\n",
        "\n",
        "#  Final input and target\n",
        "X = df.drop('Label', axis=1).values\n",
        "y = df['Label'].values\n",
        "\n",
        "print(\"‚úÖ Preprocessing done. Shape of data:\", X.shape)\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "import gym\n",
        "from gym import spaces\n",
        "import numpy as np\n",
        "\n",
        "class IDSDetectionEnv(gym.Env):\n",
        "    def __init__(self, X, y):\n",
        "        super(IDSDetectionEnv, self).__init__()\n",
        "        self.X = X\n",
        "        self.y = y\n",
        "        self.current_index = 0\n",
        "\n",
        "        # Action space: 0 (BENIGN), 1 (ATTACK)\n",
        "        self.action_space = spaces.Discrete(2)\n",
        "\n",
        "        # Observation space: one data row\n",
        "        self.observation_space = spaces.Box(low=np.min(X), high=np.max(X), shape=(X.shape[1],), dtype=np.float32)\n",
        "\n",
        "    def reset(self):\n",
        "        self.current_index = 0\n",
        "        return self.X[self.current_index]\n",
        "\n",
        "    def step(self, action):\n",
        "        correct_label = self.y[self.current_index]\n",
        "        reward = 1 if action == correct_label else -1\n",
        "\n",
        "        self.current_index += 1\n",
        "        done = self.current_index >= len(self.X)\n",
        "\n",
        "        next_state = self.X[self.current_index] if not done else np.zeros_like(self.X[0])\n",
        "\n",
        "        return next_state, reward, done, {}\n",
        "\n",
        "    def render(self, mode='human'):\n",
        "        pass  # No rendering needed\n"
      ],
      "metadata": {
        "id": "epLHt8EeiP6f"
      },
      "execution_count": 2,
      "outputs": []
    },
