      "cell_type": "code",
      "source": [
        "from sklearn.metrics import accuracy_score, precision_score, recall_score, f1_score\n",
        "\n",
        "# üéØ Common Metric Function\n",
        "def get_metrics(y_true, y_pred):\n",
        "    return {\n",
        "        \"Accuracy\": accuracy_score(y_true, y_pred),\n",
        "        \"Precision\": precision_score(y_true, y_pred),\n",
        "        \"Recall\": recall_score(y_true, y_pred),\n",
        "        \"F1-score\": f1_score(y_true, y_pred)\n",
        "    }\n",
        "\n",
        "# ‚öñÔ∏è Collect metrics from all four models\n",
        "comparison_metrics = {\n",
        "    \"Q-Learning\": get_metrics(y, [np.argmax(q_table[state]) for state in states]),\n",
        "    \"SARSA\": get_metrics(y, predictions),  # Already defined in SARSA block\n",
        "    \"Multi-Armed Bandit\": get_metrics(y, y_pred),  # Already defined\n",
        "    \"Policy Gradient\": get_metrics(epoch_labels, epoch_preds)  # From last epoch of training\n",
        "}\n",
        "\n",
        "# üßæ Print in table format\n",
        "import pandas as pd\n",
        "\n",
        "df_metrics = pd.DataFrame(comparison_metrics).T  # transpose for readability\n",
        "print(\"\\nüìä Comparison of RL-Based Classifiers:\")\n",
        "print(df_metrics.round(4))\n",
        "\n",
        "# üìä Optional: Visualization\n",
        "df_metrics.plot(kind='bar', figsize=(10, 6), colormap='Set3', rot=0)\n",
        "plt.title(\"üìä RL Classifier Comparison\")\n",
        "plt.ylabel(\"Score\")\n",
        "plt.ylim(0, 1.05)\n",
        "plt.legend(loc='lower right')\n",
        "plt.grid(True, axis='y', linestyle='--', alpha=0.7)\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
