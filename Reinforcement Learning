    {
      "cell_type": "code",
      "source": [
        "\n",
        "import numpy as np\n",
        "from sklearn.cluster import KMeans\n",
        "from sklearn.metrics import classification_report\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "\n",
        "# üé≤ Step 1: Discretize the features\n",
        "n_states = 100\n",
        "kmeans = KMeans(n_clusters=n_states, random_state=42)\n",
        "states = kmeans.fit_predict(X)\n",
        "\n",
        "# üéØ Step 2: Setup\n",
        "n_actions = 2  # 0 = BENIGN, 1 = ATTACK\n",
        "q_table = np.zeros((n_states, n_actions))\n",
        "\n",
        "# üîß Hyperparameters\n",
        "alpha = 0.1\n",
        "gamma = 0.9\n",
        "epsilon = 0.1\n",
        "episodes = 5\n",
        "\n",
        "# üìò Step 3: SARSA Training\n",
        "episode_rewards = []\n",
        "for ep in range(episodes):\n",
        "    ep_reward = 0\n",
        "    for i in range(len(states)):\n",
        "        state = states[i]\n",
        "        action = np.argmax(q_table[state]) if np.random.rand() > epsilon else np.random.randint(n_actions)\n",
        "\n",
        "        reward = 1 if action == y[i] else -1\n",
        "        ep_reward += reward\n",
        "\n",
        "        # Take next action using current policy\n",
        "        next_state = state  # No environment transition\n",
        "        next_action = np.argmax(q_table[next_state]) if np.random.rand() > epsilon else np.random.randint(n_actions)\n",
        "\n",
        "        # SARSA Update Rule\n",
        "        q_table[state, action] += alpha * (\n",
        "            reward + gamma * q_table[next_state, next_action] - q_table[state, action]\n",
        "        )\n",
        "    episode_rewards.append(ep_reward)\n",
        "\n",
        "print(\"‚úÖ SARSA training completed.\")\n",
        "\n",
        "# üß™ Step 4: Evaluation\n",
        "predictions = [np.argmax(q_table[state]) for state in states]\n",
        "print(\"üìä SARSA Classifier Report:\")\n",
        "print(classification_report(y, predictions))\n",
        "\n",
        "# üé® Step 5: Visualization\n",
        "from sklearn.metrics import confusion_matrix\n",
        "\n",
        "cm = confusion_matrix(y, predictions)\n",
        "unique, counts = np.unique(predictions, return_counts=True)\n",
        "\n",
        "fig, axes = plt.subplots(1, 3, figsize=(18, 5))\n",
        "\n",
        "# Confusion Matrix\n",
        "sns.heatmap(cm, annot=True, fmt='d', cmap='Blues',\n",
        "            xticklabels=[\"BENIGN\", \"ATTACK\"],\n",
        "            yticklabels=[\"BENIGN\", \"ATTACK\"], ax=axes[0])\n",
        "axes[0].set_title(\"üîç Confusion Matrix (SARSA)\")\n",
        "axes[0].set_xlabel(\"Predicted\")\n",
        "axes[0].set_ylabel(\"True\")\n",
        "\n",
        "# Class Distribution\n",
        "sns.barplot(x=[\"BENIGN\", \"ATTACK\"], y=counts, palette=\"magma\", ax=axes[1])\n",
        "axes[1].set_title(\"üì¶ Class Distribution (SARSA)\")\n",
        "axes[1].set_ylabel(\"Count\")\n",
        "\n",
        "# Reward Plot\n",
        "axes[2].plot(range(len(episode_rewards)), episode_rewards, marker='o', color='purple')\n",
        "axes[2].set_title(\"üìà Cumulative Reward per Episode (SARSA)\")\n",
        "axes[2].set_xlabel(\"Episode\")\n",
        "axes[2].set_ylabel(\"Total Reward\")\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
      "metadata": {
