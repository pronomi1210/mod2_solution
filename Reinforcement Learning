      "cell_type": "code",
      "source": [
        "#Q learning\n",
        "import numpy as np\n",
        "from sklearn.cluster import KMeans\n",
        "from collections import defaultdict\n",
        "import random\n",
        "\n",
        "# Step 1: Discretize the features\n",
        "n_states = 100  # Number of discrete states to approximate\n",
        "kmeans = KMeans(n_clusters=n_states, random_state=42)\n",
        "states = kmeans.fit_predict(X)\n",
        "\n",
        "# Step 2: Q-Learning Setup\n",
        "n_actions = 2  # 0 = BENIGN, 1 = ATTACK\n",
        "q_table = np.zeros((n_states, n_actions))\n",
        "\n",
        "# Hyperparameters\n",
        "alpha = 0.1      # Learning rate\n",
        "gamma = 0.9      # Discount factor\n",
        "epsilon = 0.1    # Exploration rate\n",
        "episodes = 5     # You can increase this for better training\n",
        "\n",
        "# Step 3: Q-Learning\n",
        "for ep in range(episodes):\n",
        "    for i in range(len(states)):\n",
        "        state = states[i]\n",
        "        action = np.argmax(q_table[state]) if np.random.rand() > epsilon else np.random.randint(n_actions)\n",
        "\n",
        "        reward = 1 if action == y[i] else -1  # Reward signal\n",
        "\n",
        "        next_state = state  # No environment transition here\n",
        "        best_next_action = np.argmax(q_table[next_state])\n",
        "\n",
        "        # Q-Value Update\n",
        "        q_table[state, action] = q_table[state, action] + alpha * (\n",
        "            reward + gamma * q_table[next_state, best_next_action] - q_table[state, action]\n",
        "        )\n",
        "\n",
        "print(\"‚úÖ Q-Learning training completed.\")\n",
        "\n",
        "# Step 4: Evaluate\n",
        "predictions = [np.argmax(q_table[state]) for state in states]\n",
        "\n",
        "from sklearn.metrics import classification_report\n",
        "print(\"üìä Q-Learning Classifier Report:\")\n",
        "print(classification_report(y, predictions))\n"
      ],
